<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">
   <localRepository />
   <interactiveMode />
   <usePluginRegistry />
   <offline>false</offline>
   <pluginGroups />
   <servers />
   <mirrors />
   <proxies />
   <profiles>
      <profile>
         <id>mvn-eclipse</id>
         <pluginRepositories>
            <pluginRepository>
               <id>archiva</id>
               <url>http://hagrid.mda.ca/archiva/repository/internal</url>
               <releases>
                  <enabled>true</enabled>
               </releases>
               <snapshots>
                  <enabled>true</enabled>
               </snapshots>
            </pluginRepository>
         </pluginRepositories>
      </profile>
   </profiles>
</settings>
